config_version=5

[application]

config/name="Tenacious Tempest"
run/main_scene="res://scenes/game/game.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Globals="*res://globals/globals.gd"
Player="*res://globals/player.gd"
